    "@babel/preset-typescript": "7.3.3",
    "@types/jest": "22.2.3",
    "@typescript-eslint/eslint-plugin": "2.11.0",
    "@typescript-eslint/parser": "2.11.0",
    "babel-loader": "8.0.6",
    "concurrently": "5.1.0",
    "coveralls": "3.0.9",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-prettier": "3.1.1",
    "fork-ts-checker-webpack-plugin": "3.0.1",
    "jest": "22.4.3",
